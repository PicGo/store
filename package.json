{
  "name": "@picgo/store",
  "version": "2.2.1",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "dev": "rimraf ./dist/* && tsc -w -p .",
    "build": "rimraf ./dist/* && rollup -c",
    "lint": "eslint src/**",
    "test": "vitest run --coverage",
    "cz": "git-cz",
    "release": "bump-version",
    "cover": "cat ./coverage/lcov.info | coveralls",
    "prepare": "husky",
    "commitlint": "commitlint --edit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Molunerfinn/typescript-node-template.git"
  },
  "author": "Molunerfinn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Molunerfinn/typescript-node-template/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./node_modules/@picgo/bump-version/.cz-config.js"
    }
  },
  "commitlint": {
    "extends": [
      "./node_modules/@picgo/bump-version/commitlint-picgo"
    ]
  },
  "homepage": "https://github.com/Molunerfinn/typescript-node-template#readme",
  "devDependencies": {
    "@commitlint/cli": "^20.3.0",
    "@picgo/bump-version": "^2.0.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/bson": "^4.0.1",
    "@types/graceful-fs": "^4.1.3",
    "@types/lodash": "^4.14.182",
    "@types/node": "^22.19.3",
    "@types/write-file-atomic": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@vitest/coverage-v8": "^4.0.16",
    "commitizen": "^4.3.1",
    "conventional-changelog": "^3.1.18",
    "coveralls": "^3.0.9",
    "cz-customizable": "^7.5.1",
    "eslint": "8.22.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-standard": "5.0.0",
    "husky": "^9.1.7",
    "rimraf": "^3.0.2",
    "rollup": "^2.78.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.33.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@commonify/lowdb": "^3.0.0",
    "@commonify/steno": "^2.1.0",
    "comment-json": "^4.2.3",
    "fflate": "^0.8.2",
    "lodash": "^4.17.21",
    "lodash-id": "^0.14.0",
    "write-file-atomic": "^7.0.0"
  }
}
